{
  "$schema": "https://github.com/Azure/data-api-builder/releases/download/v1.3.19/dab.draft.schema.json",
  "data-source": {
    "database-type": "mssql",
    "connection-string": "",
    "options": {
      "set-session-context": false
    }
  },
  "runtime": {
    "rest": {
      "enabled": true,
      "path": "/rest",
      "request-body-strict": true
    },
    "graphql": {
      "enabled": true,
      "path": "/graphql",
      "allow-introspection": true
    },
    "host": {
      "cors": {
        "origins": [],
        "allow-credentials": false
      },
      "authentication": {
        "provider": "StaticWebApps"
      },
      "mode": "production"
    }
  },
  "entities": {
    "Config": {
      "source": {
        "object": "dbo.Config",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "Config",
          "plural": "Configs"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "ImageData": {
      "source": {
        "object": "dbo.ImageData",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "ImageData",
          "plural": "ImageDatas"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    },
    "SessionID": {
      "source": {
        "object": "dbo.SessionID",
        "type": "table"
      },
      "graphql": {
        "enabled": true,
        "type": {
          "singular": "SessionID",
          "plural": "SessionIDs"
        }
      },
      "rest": {
        "enabled": true
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            {
              "action": "*"
            }
          ]
        }
      ]
    }
  }
}